            <RelativeLayout
                android:id="@+id/phoneNumItem"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:layout_marginTop="@dimen/dp_10"
                android:background="@drawable/selector_bg_white_square">

                <ImageView
                    android:id="@+id/phoneNumMoreIv"
                    android:layout_width="@dimen/dp_15"
                    android:layout_height="@dimen/dp_15"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:src="@drawable/more"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/phoneNumFlagTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_16"
                    android:text="手机绑定"
                    android:textColor="@color/tc"
                    android:textSize="@dimen/sp_15" />

                <TextView
                    android:id="@+id/phoneNumValueTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_16"
                    android:layout_marginEnd="@dimen/dp_8"
                    android:layout_toStartOf="@+id/phoneNumMoreIv"
                    android:layout_toEndOf="@+id/phoneNumFlagTv"
                    android:gravity="end"
                    android:textColor="@color/tc"
                    android:textSize="@dimen/sp_15"
                    tools:text="15285536453" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/safeItem"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_45"
                android:layout_marginTop="@dimen/dp_10"
                android:background="@drawable/selector_bg_white_square">

                <ImageView
                    android:id="@+id/safeMoreIv"
                    android:layout_width="@dimen/dp_15"
                    android:layout_height="@dimen/dp_15"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:src="@drawable/more"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/safeFlagTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_16"
                    android:text="安全中心"
                    android:textColor="@color/tc"
                    android:textSize="@dimen/sp_15" />

                <TextView
                    android:id="@+id/safeValueTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_16"
                    android:layout_marginEnd="@dimen/dp_8"
                    android:layout_toStartOf="@+id/safeMoreIv"
                    android:layout_toEndOf="@+id/safeFlagTv"
                    android:gravity="end"
                    android:textColor="@color/tc"
                    android:textSize="@dimen/sp_15" />
            </RelativeLayout>


